
<MD3Card>
    padding: 4
    size_hint: None, None
    size: "250dp", "150dp"
    md_bg_color: [0, 0.9, 0.9, 1]

    MDRelativeLayout:

        MDLabel:
            id: label
            text: root.text
            color: "black"
